class Solution:
    def sortList(self, head: Optional[ListNode]) -> Optional[ListNode]:
        if not head:
            return head
        values = []
        current = head
        while current:
            values.append(current.val)
            current = current.next
        values.sort()
        current = head
        for val in values:
            current.val = val
            current = current.next
        return head
        
        
